# mysql Database Settings
jdbc.driver = org.mariadb.jdbc.Driver
jdbc.username = <%= link('mariadb_link').p('admin_username')%>
jdbc.pwd = <%= link('mariadb_link').p('admin_password')%>
jdbc.url = jdbc:mysql://<%= link('mariadb_link').instances[0].address%>:<%= link('mariadb_link').p('mysql_port')%>
jdbc.host = <%= link('mariadb_link').instances[0].address%>
jdbc.port = <%= link('mariadb_link').p('mysql_port')%>
